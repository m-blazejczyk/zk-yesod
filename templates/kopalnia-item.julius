if(#{isEdit}) {
  $(document).ready(function() {
      $('#tytul').editable();

      $('#rodzaj').editable(#{rawJS $ decodeUtf8 rodzajeToJson});

      $('#url').editable({
        display: function(value, response) {
         if(value.trim()=='') {
             $(this).html('link do zawartości [brak]');
           } else {
             $(this).html('link do zawartości [ustawiony]'); 
           }
        }
      });
  });
}